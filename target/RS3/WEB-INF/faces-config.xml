<?xml version="1.0" encoding="UTF-8"?> 
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xi="http://www.w3.org/2001/XInclude"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
   
   
 
    
    
   <navigation-rule>
	   <from-view-id>/*</from-view-id>
	   <navigation-case>
		   <from-outcome>cartview</from-outcome>
		   <to-view-id>/shopping/cartvaluedview.xhtml</to-view-id>
	   </navigation-case> 

   </navigation-rule>
   

   <navigation-rule>
	   <from-view-id>/*</from-view-id>
	   <navigation-case>
		   <from-outcome>availview</from-outcome>
		   <to-view-id>/testdef.xhtml</to-view-id>
	   </navigation-case>   
   </navigation-rule>
   
   <navigation-rule>
	   <from-view-id>/*</from-view-id>
	   <navigation-case>
		   <from-outcome>purchaseview</from-outcome>
		   <to-view-id>/shopping/PurchaseView.xhtml</to-view-id>
	   </navigation-case>   
   </navigation-rule>
   
   <navigation-rule>
	   <from-view-id>/*</from-view-id>
	   <navigation-case>
		   <from-outcome>order</from-outcome>
		   <to-view-id>/shopping/PurchaseView.xhtml</to-view-id>
	   </navigation-case>   
   </navigation-rule>
   
      <navigation-rule>
	   <from-view-id>/*</from-view-id>
	   <navigation-case>
		   <from-outcome>purchasedetailview</from-outcome>
		   <to-view-id>/shopping/PurchaseDetailView.xhtml</to-view-id>
	   </navigation-case>   
   </navigation-rule>
   
   
 
</faces-config>