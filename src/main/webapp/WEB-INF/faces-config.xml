<?xml version="1.0" encoding="UTF-8"?> 
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xi="http://www.w3.org/2001/XInclude"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
   
   
 
    
    
   <navigation-rule>
	   <from-view-id>/*</from-view-id>
	   <navigation-case>
		   <from-outcome>cartview</from-outcome>
		   <to-view-id>/shopping/CartView.xhtml</to-view-id>
	   </navigation-case> 

   </navigation-rule>
   

   <navigation-rule>
	   <from-view-id>/*</from-view-id>
	   <navigation-case>
		   <from-outcome>availabilityview</from-outcome>
		   <to-view-id>/shopping/AvailabilityView.xhtml</to-view-id>
	   </navigation-case>   
   </navigation-rule>
   
   <navigation-rule>
	   <from-view-id>/*</from-view-id>
	   <navigation-case>
		   <from-outcome>configview</from-outcome>
		   <to-view-id>/shopping/ConfigView.xhtml</to-view-id>
	   </navigation-case>   
   </navigation-rule>
   
   <navigation-rule>
	   <from-view-id>/*</from-view-id>
	   <navigation-case>
		   <from-outcome>orderview</from-outcome>
		   <to-view-id>/shopping/OrderView.xhtml</to-view-id>
	   </navigation-case>   
   </navigation-rule>
   
  <navigation-rule>
	   <from-view-id>/*</from-view-id>
	   <navigation-case>
		   <from-outcome>ofertaview</from-outcome>
		   <to-view-id>/shopping/OfertaView.xhtml</to-view-id>
	   </navigation-case>   
   </navigation-rule>
   
   <navigation-rule>
	   <from-view-id>/*</from-view-id>
	   <navigation-case>
		   <from-outcome>home</from-outcome>
		   <to-view-id>/home/Home.xhtml</to-view-id>
	   </navigation-case>   
   </navigation-rule>
   

   
   
 
</faces-config>